  "query": "\n        SELECT t.tag_id, t.name, t.type,\n               COUNT(ft.file_hash) as file_count\n        FROM Tags t\n        LEFT JOIN FileTags ft ON t.tag_id = ft.tag_id\n        WHERE t.name LIKE ?\n        GROUP BY t.tag_id\n        ORDER BY COUNT(ft.file_hash) DESC, t.name ASC\n        LIMIT ?\n        ",
